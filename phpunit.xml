        bootstrap="./vendor/autoload.php"
        colors="true"
>
    <testsuites>
        <testsuite name="DSTU1-Primitives">
            <directory>./src/DCarbone/PHPFHIRGenerated/DSTU1/PHPFHIRTests</directory>
