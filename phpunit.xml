            <directory>./src/DCarbone/PHPFHIRGenerated/DSTU1/PHPFHIRTests/FHIRElement</directory>
            <exclude>./src/DCarbone/PHPFHIRGenerated/DSTU1/PHPFHIRTests/FHIRElement/FHIRBackboneElement</exclude>
        </testsuite>
        <testsuite name="DSTU1-BackboneElements">
            <directory>./src/DCarbone/PHPFHIRGenerated/DSTU1/PHPFHIRTests/FHIRElement/FHIRBackboneElement</directory>
            <exclude>./src/DCarbone/PHPFHIRGenerated/DSTU1/PHPFHIRTests/FHIRElement/FHIRBackboneElement/FHIRResource</exclude>
        </testsuite>
        <testsuite name="DSTU1-Resources">
